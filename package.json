{
  "name": "ts-vite-project",
  "version": "0.0.2",
  "description": "Template for projects based typescript, vite, vitest, eslint, prettier, electron",
  "keywords": [],
  "main": "main.js",
  "repository": "https://github.com/fwmakc/ts-vite-project.git",
  "author": "fwmakc <fwmakc@mail.ru>",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite/config.dev.mjs",
    "build": "tsc && vite build --config vite/config.prod.mjs",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,mts,cts}\" --fix",
    "test": "vitest --config ./vitest.config.js",
    "electron:dev": "electron .",
    "electron:make": "electron-forge make",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "^37.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^2.3.2",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {}
}
